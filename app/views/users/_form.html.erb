<%= form_for(@user) do |f| %> 
    <%= f.label :name %> 
    <%= f.text_field :name %> <br>
    <%= f.label :email %> 
    <%= f.text_field :email %> <br>  
    <%= f.label :password %> 
    <%= f.password_field :password_digest %><br><br> 
    <%= f.hidden_field :authenticity_token, form_authenticity_token %> 
    <%= f.submit %> 
<% end %> 