<%= render 'layouts/errors', object: @player %>
        

<%= form_for(@player) do |f| %> 
  
  
        <%= f.label :name %> 
        <%= f.text_area :name %> <br>
        <%= f.label :date_of_birth %> 
        <%= f.number_field :date_of_birth %> <br>
    
        <%= f.hidden_field :team_id %>
        <%= f.hidden_field :user_id %>   
    
        <%= f.submit %> 
    <% end %> 