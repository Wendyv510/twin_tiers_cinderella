<%= form_for(@player) do |f| %> 
  
  <%= render 'layouts/errors', object: @player %>
        
        <%= f.label :name %> 
        <%= f.text_area :name %> <br>
        <%= f.label :age %> 
        <%= f.number_field :age %> <br>
    
        <%= f.hidden_field :team_id %>
        <%= f.hidden_field :user_id %>  
    
        <%= f.submit %> 
    <% end %> 