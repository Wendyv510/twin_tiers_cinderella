<%= render 'layouts/errors', object: @player %>
        

    <%= form_for(@player) do |f| %> 
  
  
        <%= f.label :name %> 
        <%= f.text_area :name %> <br>
        <%= f.label :date_of_birth %> 
        <%= f.date_select :date_of_birth, start_year: Date.today.year - 5, end_year: Date.today.year - 18 %> <br>
    
         <%= f.submit %> 
    <% end %> 