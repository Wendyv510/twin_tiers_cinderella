<%= render 'layouts/errors', object: @player_team %>
        

<%= form_for(@player_team) do |f| %> 
  
       <%= select_tag "Player", options_from_collection_for_select(Player.all,"id","name") %>

        <%= f.label :position %> 
        <%= f.text_area :position %> <br>
        <%= f.label :year %> 
        <%= f.number_field :year %> <br>

        <%= f.hidden_field :team_id %> 
    
          
    
        <%= f.submit %> 
    <% end %> 